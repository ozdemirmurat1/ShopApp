@model ShopApp2.Presentation.Models.ProductListModel

<div class="row">
    <div class="col-md-3">
        @await Component.InvokeAsync("CategoryList")
    </div>
    <div class="col-md-9">
       
        <div class="row">
            @foreach (var p in Model.Products)
            {
                @await Html.PartialAsync("_product", p)
            }
        </div>
        <div page-model="@Model.PageInfo">

        </div>
    </div>
</div>


